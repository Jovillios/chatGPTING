{
  "manifest_version": 3,
  "name": "ChatGPTING",
  "version": "0.2",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "description": "Cette extension permet de remplir des livrables pour l'UE TING grâce à ChatGPT",
  "content_scripts": [
    {
      "matches": ["https://chat.openai.com/*"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ]
}
